NAMESPACE example

PROCEDURE @add
INTERFACE
  USING a INT, b INT
  GIVING c INT
IMPLEMENTATION
  ADD a TO b GIVING c
END

PROCEDURE @multiply
INTERFACE
  USING a INT, b INT
  GIVING c INT
IMPLEMENTATION
  MULTIPLY a BY b GIVING c
END

PROCEDURE arithmetic
INTERFACE
  USING a INT, b INT
  GIVING sum INT, product INT
IMPLEMENTATION
  CALL @add USING a, b GIVING sum
  CALL @multiply USING a, b GIVING product
END

FUNCTION arithmetic_report() REPORT-HANDLER =
  REPORT-HANDLER arithmetic
